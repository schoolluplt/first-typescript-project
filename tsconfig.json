{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "baseUrl": ".",
    "paths": {},
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "es2020"
    ]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
  